				"keyword.operator.void"
			],
			"settings": {
				"foreground": "#c79adf"
			}
		},
		{
			"name": "Storage types / modifiers",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.function",
				"storage.type.class",
				"storage.type.interface",
				"storage.type.enum",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#c79adf"
			}
		},
		{
			"name": "Regex symbols and operators",
			"scope": [
				"keyword.operator.or.regexp",
				"keyword.operator.negation.regexp",
				"keyword.control.anchor.regexp"
			],
			"settings": {
				"foreground": "#c79adf"
			}
		},
		{
			"name": "HTML attributes",
			"scope": [
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.xml"
			],
			"settings": {
				"foreground": "#c79adf",
				"fontStyle": "italic"
			}
		},

		// ── Variables, Class Names ───────────────────────────────
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other",
				"variable.other.readwrite",
				"variable.other.object",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#e4f0e7"
			}
		},
		{
			"name": "Class / type names",
			"scope": [
				"entity.name.type.class",
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#e4f0e7"
			}
		},

		// ── Editor Foreground (operators, punctuation) ──────────
		{
			"name": "Editor foreground / default text",
			"scope": [
				"source",
				"meta.brace",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly",
				"punctuation.accessor",
				"punctuation.separator",
				"punctuation.terminator",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#c6d3c9"
			}
		},

		// ── Markdown Text, HTML Text ────────────────────────────
		{
			"name": "Markdown text / HTML text",
			"scope": [
				"text.html",
				"text.html.basic",
				"text.html.markdown",
				"markup.paragraph.markdown"
			],
			"settings": {
				"foreground": "#a1b4a8"
			}
		},

		// ── Markdown Formatting ─────────────────────────────────
		{
			"name": "Markdown bold",
			"scope": [
				"markup.bold",
				"markup.bold.markdown",
				"punctuation.definition.bold.markdown"
			],
			"settings": {
				"foreground": "#e4f0e7",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown italic",
			"scope": [
				"markup.italic",
				"markup.italic.markdown",
				"punctuation.definition.italic.markdown"
			],
			"settings": {
				"foreground": "#c79adf",
				"fontStyle": "italic"
			}
		},

		// ── String Escape Characters ────────────────────────────
		{
			"name": "String escape characters",
			"scope": [
				"constant.character.escape",
				"constant.character.escape.ts",
				"constant.character.escape.js"
			],
			"settings": {
				"foreground": "#e05a4f"
			}
		},

		// ── Decorators / Annotations ────────────────────────────
		{
			"name": "Decorators / Annotations",
			"scope": [
				"meta.decorator",
				"meta.decorator entity.name.function",
				"meta.annotation"
			],
			"settings": {
				"foreground": "#d6c96d",
				"fontStyle": "italic"
			}
		},

		// ── Type Annotations ────────────────────────────────────
		{
			"name": "Type annotations",
			"scope": [
				"support.type.primitive",
				"entity.name.type.ts",
				"support.type.builtin.ts"
			],
			"settings": {
				"foreground": "#d6c96d"
			}
		},

		// ── Diff / Invalid / Deprecated ─────────────────────────
		{
			"name": "Inserted text (diff)",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#64c27c"
			}
		},
		{
			"name": "Deleted text (diff)",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#e05a4f"
			}
		},
		{
			"name": "Changed text (diff)",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#d6a657"
			}
		},
		{
			"name": "Invalid / Illegal",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#e05a4f",
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Deprecated",
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "#d6a657",
				"fontStyle": "strikethrough"
			}
		}
	]
}
